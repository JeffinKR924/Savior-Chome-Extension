{
  "manifest_version": 3,

  "name": "Savior Plugin",
  "description": "This extension will save your webpage",
  "version": "1.0",

  "action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },

  "background" : {
    "service_worker": "eventPage.js",
    "type": "module"
  },
  "permissions": [
   "activeTab",
   "declarativeContent",
   "storage",
   "contextMenus",
   "tabs",
   "favicon"
   ],
   "host_permissions": [
    "<all_urls>"
  ],
   "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; img-src chrome://favicon"
   }
}